<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MAUI_Aeropuertos.Views.SearchPage"
             Title="Pablo Yánez">

    <StackLayout Padding="20">

        <Entry x:Name="txtSearch" Placeholder="Ingrese un país" />

        <Button Text="Buscar" x:Name="btnSearch" Clicked="OnSearchClicked" />


        <Button Text="Limpiar" x:Name="btnClear" Clicked="OnClearClicked" />

        <Label x:Name="lblResult" />
    </StackLayout>
</ContentPage>
